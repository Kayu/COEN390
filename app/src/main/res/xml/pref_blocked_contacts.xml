<?xml version="1.0" encoding="utf-8"?>


<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">

    <com.example.kayuho.coen390.Model.BlockedContactsEditTextPreference
        android:capitalize="words"
        android:defaultValue="Please Enter the Phone Number of the Contact you wish to Block"
        android:inputType="textCapWords"
        android:key="blocked_contacts"
        android:maxLines="1"
        android:selectAllOnFocus="true"
        android:singleLine="true"
        android:title="Add Phone # of Contact to Block"/>

    <com.example.kayuho.coen390.Model.DeleteBlockedContactConfirmationPreference
        android:capitalize="words"
        android:defaultValue="Delete blocked contact"
        android:inputType="textCapWords"
        android:key="delete_blocked_contact"
        android:maxLines="1"
        android:selectAllOnFocus="true"
        android:singleLine="true"
        android:title="Delete Blocked Contact" />

    <com.example.kayuho.coen390.Model.DeleteAllContactsPreference
        android:dialogMessage="Are you sure you wish to remove all numbers from the blocked list?"
        android:key="delete_all_contacts"
        android:title="Delete All Currently Blocked Contact" />



        <com.example.kayuho.coen390.Model.ListBlockedContactsTextPref
            android:defaultValue="-1"
            android:key="blocked_list"
            android:title="See Blocked Contacts"/>



</PreferenceScreen>